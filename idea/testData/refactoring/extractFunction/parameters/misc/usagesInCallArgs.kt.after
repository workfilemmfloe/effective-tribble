// PARAM_TYPES: A
// PARAM_TYPES: kotlin.Int
// PARAM_TYPES: kotlin.Int
// PARAM_TYPES: kotlin.Int
// SIBLING:
public class A() {
    fun bar(a: Int, b: Int): Int {
        return a + b
    }
}

fun i(a: A, t: Int, u: Int, x: Int): Int {
    return a.bar(t - x, u + x)
}

fun foo(a: A, x: Int): Int {
    val t = 10
    val u = 20
    return i(a, t, u, x)
}
