//file
import java.io.*;

public class C {
    int foo() {
        try(InputStream stream = new FileInputStream("foo")) {
            while(true) {
                int c = stream.read();
                if (c < 0 || c > 127) return c;
            }
        }
        catch (IOException e) {
            System.out.println(e);
            return -1;
        }
    }
}
